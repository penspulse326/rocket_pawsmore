{
  "trailingComma": "es5",
  "semi": true,
  "tabWidth": 2,
  "singleQuote": true,
  "printWidth": 100,
  "jsxSingleQuote": true,
  "plugins": ["prettier-plugin-tailwindcss"],
  "importOrder": [
    "external",
    "builtin",
    "internal",
    "parent",
    "sibling",
    "index",
    "object",
    "type"
  ],
  "importGroups": [
    {
      "pattern": "react",
      "group": "external",
      "position": "before"
    }
  ],
  "importOrderSeparation": true
}
